{
  "presets": [
    ["@babel/preset-env", { 
      "targets": {
        "browsers": "last 2 versions"
      },
      "loose": true,
      "modules": false
    }],
    "@babel/preset-react",
  ],
  "plugins": [
    "@babel/plugin-transform-modules-commonjs",
    "babel-plugin-transform-class-properties",
    "@babel/plugin-transform-regenerator"
  ]
}
